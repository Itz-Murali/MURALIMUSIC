{
    "name": "CUTE X MUSIC",
    "description": "Cutex Music Is A Pyrogram Based Telegram VC streaming Bot",
    "logo": "https://telegra.ph/file/a14bb1aa5cf32f8dba095.jpg",
    "keywords": [
        "python3",
        "telegram",
        "bot",
        "CUTE",
        "CUTEXMUSIC",
        "cutexmusicbot",
        "pyrogram"
    ],
    "stack": "container",
    "env": {
        "API_ID": {
            "description": "Get this value from https://my.telegram.org",
            "value": "",
            "required": true
        },
        "API_HASH": {
            "description": "Get this value from https://my.telegram.org",
            "value": "",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "A Bot's token from @Botfather on Telegram",
            "value": "",
            "required": true
        },
        "OWNER_ID": {
            "description": "The user ID(s) of user(s) whom you would like to add as an OWNER. Multiple values shall be separated with a space.",
            "value": "6844821478",
            "required": true
        },
        "MONGO_DB_URI": {
            "description": "Mongo DB URL",
            "value": "",
            "required": true
        },
        "MURALI_DB": {
            "description": "A Database URL For Cutex Music. If you don't know this, leave it blank.",
            "value": "",
            "required": false
        },
        "STRING_SESSION": {
            "description": "A Pyrogram Session. Get It From Here @Shalini_string_Bot.",
            "value": "",
            "required": true
        },
        "HEROKU_API_KEY": {
            "description": "Your Heroku account's API key",
            "value": "",
            "required": false
        },
        "HEROKU_APP_NAME": {
            "description": "Your Heroku app name",
            "value": "",
            "required": false
        },
        "LOG_GROUP_ID": {
            "description": "Your Log Group ID. Add your bot and promote as an admin with full rights! Use only Group. Please don't use Channel ID.",
            "value": "",
            "required": true
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "eco"
        }
    }
}

